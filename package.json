{
  "name": "deepseek-vscode-ext",
  "displayName": "DeepSeek",
  "description": "DeepSeek Assistant VS Code Extension",
  "version": "0.0.1",
  "engines": {
  "vscode": "^1.97.0"
},
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "DeepSeek.start",
        "title": "DeepSeek Start"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "author": "sai teja",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/saitejab2001/DeepSeek-VSCode-Extension"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "ollama": "^0.5.12"
  }
}
